#!/usr/bin/env node

'use strict';

process.title = 'ember';

require('../lib/cli')({
  cliArgs: process.argv.slice(2),
  errorStream: process.stderr,
  inputStream: process.stdin,
  outputStream: process.stdout,
}).then(function(result) {
  require('exit')(typeof result === 'object' ? result.exitCode : result);
}, function(error) {
  console.error(error);
  process.exit(-1);
});
